{
  "extends": ["airbnb", "prettier", "plugin:react-redux/recommended"],
  "plugins": [
    "react",
    "prettier",
    "react-redux"
  ],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "no-shadow": "off",
    "react-redux/connect-prefer-named-arguments": "off",
    "react-redux/prefer-separate-component-file": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "import/no-unresolved": "off"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}